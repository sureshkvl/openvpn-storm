{
  "name": "openvpn-storm",
  "description": "openvpn-storm is a easy to use module that exposes endpoints to configure openvpn on any linux system",
  "author": "Clearpath Plugins Team",
  "version": "0.5.3",
  "repository": {
    "type": "git",
    "url": "git://github.com/stormstack/openvpn-storm.git"
  },
  "dependencies": {
    "async": "0.9.0",
    "json-schema": "0.2.0",
    "lazy": "1.0.11",
    "node-uuid": "1.3.3",
    "path": "0.4.9",
    "stormdata": "1.2.2",
    "stormregistry": "1.3.2",
    "stormservice": "1.0.5",
    "fmerge":"1.2.0",
    "deep-diff": "*",
    "utils": "*"
  },
  "engines": {
    "node": ">=0.6.x"
  },
  "stormflash": {
    "packages": [
      "apt-get://openvpn",
      "apt-get://spawnvpn"
    ],
    "services": [
      "openvpn.*"
    ],
    "plugin": "lib/plugin"
  },
  "main": "./lib/openvpn-service",
  "config": {
    "logfile": "/var/log/openvpn-storm.log",
    "storm": {
      "plugins": [
        "lib/plugin"
      ]
    }
  },
  "scripts": {
    "prepublish": "mkdir -p lib; coffee -o lib -c src"
  },
  "devDependencies": {},
  "optionalDependencies": {},
  "keywords": [
    "openvpn",
    "vpn"
  ],
  "contributors": [
    "Ravi Chunduru",
    "Peter.K.Lee <plee@clearpathnet.com",
    "Geetha R <geethar@calsoftlabs.com",
    "Suresh Kumar <sureshkumar.s@calsoftlabs.com",
    "Sivaprasath Busa <sbusa@clearpathnet.com>"
  ]
}
