{
  "name": "openvpn-storm",
  "description": "openvpn-storm is a easy to use module that exposes endpoints to configure openvpn on any linux system",
  "author": "Ravi Chunduru",
  "version": "0.1.15",
  "repository": {
     "type": "git",
     "url": "git://github.com/stormstack/openvpn-storm.git"
  },
  "dependencies": {
    "stormagent":"x",
    "fileops":"0.1.3",
    "json-schema": "0.2.0",
    "dirty":"0.9.x",
    "lazy":"1.0.x",
    "node-uuid": "1.3.x",
    "path":"x"
  },
  "engines": {
    "node": ">=0.6.x"
  },
  "stormflash": {
    "packages": [
      "apt://repo.dev.intercloud.net/openvpn",
      "apt://repo.dev.intercloud.net/spawnvpn"
    ],
    "services": [
      "openvpn.*"
    ],
    "plugin": "plugin.coffee"
  },
  "main": "./lib/openvpn-storm",
  "config": {
      "logfile": "/var/log/openvpn-storm.log",
      "storm": {
          "plugins": [
              "lib/plugin"
  		  ]
      }
  },
  "scripts": {
      "prepublish": "mkdir -p lib; coffee -o lib -c src",
      "start": "node --expose-gc lib/openvpn-storm"
  },
  "devDependencies": {},
  "optionalDependencies": {},
  "keywords": ["openvpn", "vpn"],
  "contributors": [
      "Peter.K.Lee <plee@clearpathnet.com",
      "Badri Narayanan <badrinarayananb@calsoftlabs.com",
	  "Geetha R <geethar@calsoftlabs.com",
	  "Suresh Kumar <sureshkumar.s@calsoftlabs.com",
	  "Sivaprasath Busa <sbusa@clearpathnet.com>"
   ]
}
